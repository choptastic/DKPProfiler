	<Script file="DKPProfiler.lua"/>
	<Frame name="DKPProfilerFrame" hidden="true">
		<Scripts>
			<OnLoad>
				DKPProfiler_OnLoad(self);
			</OnLoad>
			<OnEvent>
				DKPProfiler_OnEvent(self,event,...);
			</OnEvent>
		</Scripts>
	</Frame>
<GameTooltip name="DKPPKTooltip" inherits="GameTooltipTemplate">
   <Scripts>
     <Onload>
	     self:SetOwner(UIParent, "ANCHOR_NONE");
     </Onload>
   </Scripts>
</GameTooltip>


</Ui>









